{
    "name": "drissy",
    "version": "4.1.0",
    "description": "``` Install with: composer create-project hendrikeng/drissy PROJEKTNAME -s RC",
    "keywords": [
        "Craft CMS",
        "Craft",
        "Webpack",
        "Vue",
        "Tailwind",
        "Contentbuilder",
        "Scaffolding"
    ],
    "homepage": "https://github.com/hendrikeng/drissy#readme",
    "bugs": {
        "url": "https://github.com/hendrikeng/drissy/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Hendrik Werner",
        "email": "hendrik@wewereyoung.de",
        "url": "https://wewereyoung.de"
    },
    "browser": "/web/index.php",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hendrikeng/drissy.git"
    },
    "private": true,
    "scripts": {
        "dev": "webpack-dev-server --config webpack.dev.js",
        "build": "webpack --config webpack.prod.js --progress --hide-modules"
    },
    "browserslist": {
        "production": [
            "> 1%",
            "last 2 versions",
            "Firefox ESR",
            "IE 11"
        ],
        "legacyBrowsers": [
            "> 1%",
            "last 2 versions",
            "Firefox ESR",
            "IE 11"
        ],
        "modernBrowsers": [
            "last 2 Chrome versions",
            "not Chrome < 79",
            "last 2 Safari versions",
            "not Safari < 12",
            "last 2 iOS versions",
            "not iOS < 10.3",
            "last 2 Firefox versions",
            "not Firefox < 71",
            "last 2 Edge versions",
            "not Edge < 18"
        ]
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "./eslint.config.js"
        ]
    },
    "eslintIgnore": [
        "/node_modules/**",
        "/modules/**",
        "/storage/**",
        "/vendor/**",
        "/src/fonts/**",
        "/src/inlineJs/**",
        "/web/**",
        "./tailwind.config.js",
        "/src/js/workbox-catch-handler.js"
    ],
    "stylelint": {
        "extends": [
            "./stylelint.config.js"
        ],
        "ignoreFiles": [
            "node_modules/**/*",
            "modules/**/*",
            "storage/**/*",
            "vendor/**/*",
            "src/fonts/**/*",
            "src/vendor/**/*",
            "web/**/*",
            "css-gridish/**/*"
        ]
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "printWidth": 80,
        "proseWrap": "preserve",
        "requirePragma": false,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all",
        "useTabs": false,
        "overrides": [
            {
                "files": "*.json",
                "options": {
                    "printWidth": 200
                }
            }
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
