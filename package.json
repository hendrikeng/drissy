{
  "name": "drissy",
  "version": "4.0.0",
  "description": "``` Install with: composer create-project hendrikeng/drissy PROJEKTNAME -s RC",
  "keywords": [
    "Craft CMS",
    "Craft",
    "Webpack",
    "Vue",
    "Tailwind",
    "Contentbuilder",
    "Scaffolding"
  ],
  "homepage": "https://github.com/hendrikeng/drissy#readme",
  "bugs": {
    "url": "https://github.com/hendrikeng/drissy/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Hendrik Werner",
    "email": "hendrik@wewereyoung.de",
    "url": "https://wewereyoung.de"
  },
  "browser": "/web/index.php",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hendrikeng/drissy.git"
  },
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --progress --hide-modules"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "legacyBrowsers": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "modernBrowsers": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "./eslint.config.js"
    ]
  },
  "eslintIgnore": [
    "/node_modules/**",
    "/modules/**",
    "/storage/**",
    "/vendor/**",
    "/src/fonts/**",
    "/src/inlineJs/**",
    "/web/**",
    "./tailwind.config.js",
    "/src/js/workbox-catch-handler.js"
  ],
  "stylelint": {
    "extends": [
      "./stylelint.config.js"
    ],
    "ignoreFiles": [
      "node_modules/**/*",
      "modules/**/*",
      "storage/**/*",
      "vendor/**/*",
      "src/fonts/**/*",
      "src/vendor/**/*",
      "web/**/*",
      "css-gridish/**/*"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 200
        }
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "@babel/runtime-corejs3": "^7.6.0",
    "@gfx/zopfli": "^1.0.14",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "create-symlink-webpack-plugin": "^1.0.1",
    "critical": "^1.3.4",
    "critical-css-webpack-plugin": "^0.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-vue": "^5.2.3",
    "favicons-webpack-plugin": "^1.0.2",
    "file-loader": "^4.0.0",
    "git-rev-sync": "^1.12.0",
    "glob-all": "^3.1.0",
